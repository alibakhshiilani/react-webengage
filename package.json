{
  "name": "react-webengage",
  "version": "1.0.6",
  "description": "reactjs webengage",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "umd:main": "dist/umd/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "keywords": [
    "webengage",
    "reactjs",
    "reactjs webenage"
  ],
  "author": "Ali Bakhshi Ilani <info@ali-bakhshi.ir>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alibakhshiilani/react-webengage"
  },
  "bugs": {
    "url": "https://github.com/alibakhshiilani/react-webengage/issues"
  }
}
